<div id="<%= dom_id article %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Title:</strong>
    <%= article.title %>
  </p>
  <p class="my-5">
    <strong class="block font-medium mb-1">Content:</strong>
    <%= article.content %>
  </p>
  <p class="my-5">
    <strong class="block font-medium mb-1">Slug:</strong>
    <%= article.slug %>
  </p>
  <p class="my-5">
    <strong class="block font-medium mb-1">Meta description:</strong>
    <%= article.meta_description %>
  </p>
</div>
